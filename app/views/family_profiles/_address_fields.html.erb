<fieldset>

  <!-- <%= f.label :street_address, t("views.family_profiles._form.labels.street_address") %> -->
  <%= f.text_area :street_address, rows: 3, placeholder: t("views.family_profiles._address_fields.placeholders.street_address") %>

  <div class="row">
    <div class="twelve columns">
    </div>
    <div class="six columns">
      <%= f.label :city, t("views.family_profiles._form.labels.city") %>
      <%= f.text_field :city %>

      <%= f.label :state, t("views.family_profiles._form.labels.state") %>
      <%= f.text_field :state %>


      <%= f.label :zip_code, t("views.family_profiles._form.labels.zip_code") %>
      <%= f.text_field :zip_code %>

      <%= f.label :phone, t("views.family_profiles._form.labels.phone") %>
      <%= f.text_field :phone %>

      <%= f.label :email, t("views.family_profiles._form.labels.email") %>
      <%= f.text_field :email %>
    </div>
  </div>

  <!-- <p class="remove_field"><%= f.link_to_remove "Remove this Address" %></p> -->
</fieldset>
